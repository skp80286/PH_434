   "source": [
    "import numpy as np\n",
    "state0 = np.array([1,0])\n",
    "state1 = np.array([0,1])\n",
    "X = np.array([[0,1],[1,0]])\n",
    "proj0=np.outer(state0, state0)\n",
    "print(proj0)\n",
    "\n",
    "proj1=np.outer(state1, state1)\n",
    "print(proj1)\n",
    "\n",
    "U_cnot = np.kron(proj0, np.eye(2)) + np.kron(proj1, X)\n",
    "print(U_cnot)"
   ]
